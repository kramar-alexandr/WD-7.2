    OutString(-90,0,"Відправлено",true);
    OutString(1,0,"Отримано",true);
  Endformat;
  Black_Divider(0,1);  
  
//----------------------FROM location---------------------------//  

  if (RepSpec.flags[1]==0) then begin
		key = 2;
		SMr.OKFlag = 0;
	end else begin
		key = 1;
	end;  
	
