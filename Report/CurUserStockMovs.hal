  StartReportJob("Переміщення " & CurrentUser);
   rw = 1;	
	 Header(rw,"Всі переміщення для складу " & Userr.Location,1);
  EndHeader;
  
  StartFormat(20);
    OutString(20,0,"Номер",false);
    OutString(100,0,"Дата",true);
