procedure GetIHFileName(record ItemHistVc orIHr,var string tstr,string group,var date inputdate)
begin	
  record ItemHistVc tradeIHr,IHr;
  boolean TrHs,foundf;
    
    recordcopy(IHr,orIHr);
    
    if(group=="TRADE")then begin
      tradeIHr.SerialNr = IHr.SerialNr;
      tradeIHr.ArtCode = IHr.ArtCode;
      TrHs = true;
      while(loopbackkey("CodeSerialNrArtCode",tradeIHr,2,TrHs))begin
        if(tradeIHr.SerialNr!=IHr.SerialNr)then begin TrHs = false; end;
        if(tradeIHr.ArtCode!=IHr.ArtCode)then begin TrHs = false; end;
        
        if(TrHs)then begin
          if(tradeIHr.FileName=="SRVc")then begin
            TrHs = false;
            recordcopy(IHr,tradeIHr);
            foundf = true;
            inputdate = tradeIHr.TransDate;
          end;
          if(tradeIHr.FileName=="PUVc")then begin
            TrHs = false;
            recordcopy(IHr,tradeIHr);
            foundf = true;
            inputdate = tradeIHr.TransDate;
          end;
        end;
      end;
    end;
    
	  switch (IHr.FileName) begin
	    case "IVVc": tstr = USetStr(1150);
	    case "PUVc": tstr = USetStr(1157);      
	    case "ProdVc": tstr = USetStr(1162);
	    case "RetVc": tstr = USetStr(1421);
	    case "StockMovVc": tstr = USetStr(1172);
	    case "StockTakeVc": tstr = USetStr(1392);
	    case "ProdOperationVc": tstr = USetStr(1633);
	    case "SRVc": tstr = USetStr(1636);
	    
	    case "SHVc": tstr = USetStr(5003);
	    case "IVCashVc": tstr = USetStr(11675);
	    case "SDVc": tstr = USetStr(5020);
	    case "WSVc": tstr = USetStr(7650);
	    case "INTransferVc": tstr = USetStr(5040);
	  end;
	  //tstr = USetStr(1157) & ":" & IHr.InSerNr;// Edit ************************** Friday, 20 February 2015 18:26:10
	  if(group=="TRADE" and foundf)then begin
      tstr = tstr & ":";
	    tstr = tstr & IHr.TransNr;
	  end else begin
	    tstr = USetStr(1157) & ":" & IHr.InSerNr;// Edit ************************** Friday, 20 February 2015 18:26:10
	  end;
  return;	  
