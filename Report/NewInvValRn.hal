procedure PrintItemClass(record INVc INr)
begin
record DIVc DIr,DI2r;

  DIr.Code = INr.DispGroups;
  readfirstmain(DIr,1,true);
  if(blank(DIr.DispGroups))then begin
    DIr.DispGroups = "UNKNOWN";
  end else begin
    if(blank(DIr.CType))then begin
      DI2r.Code = DIr.DispGroups;
      if(readfirstmain(DI2r,1,true))then begin
        if(nonblank(DI2r.CType))then begin
          DIr.CType = DI2r.CType;
        end;
      end;
    end;
  end;
  
  OutString(200,0,INr.DispGroups,false);
  OutString(200,0,DIr.DispGroups,false);
  OutString(200,0,DIr.CType,false);
                  
return;
end;


function integer GetSign(val qty) //Edit***************************Sasha2,15:47 04.09.2014 {
begin
  integer res;
  if (qty<0) then begin
  	res = -1;
  end else begin
  	res = 1;	
  end;
  GetSign = res;
  return;
end; //Edit***************************Sasha2,15:47 04.09.2014 }


function val Min(val v1,val v2)
begin
  val res;
  
  if (v1>v2) then begin
    res = v2;
  end else begin
    res = v1;
  end;
  Min = res;
  return;
end;


