              OutString(costpos,0,"",false);
              OutString(totcostpos,0,"",false);
						end;
					end else begin
						OutVal(rempos,0,itemrem,M45Val,false);
						if(usercanaction("ViewCostPrice",false))then begin
              OutVal(costpos,0,itemcostsum/itemqty,M45Val,false);
              OutVal(totcostpos,0,itemsum,M45Val,false);	
						end;
					end;
				  	EndFormat;
				  	Gray_Divider(0,1);					
				end;
			  	print = false;
			end;
			
		end;	
	end;
	Black_Divider(2,1);
	
	if(usercanaction("ViewCostPrice",false))then begin
    StartFormat(15);
