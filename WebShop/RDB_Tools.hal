globalupdating procedure RDBBuildAllDataMn()begin  Integer wn,i,rwcnt;  record RDBRegsBlock RDBr;  row RDBRegsBlock RDBrw;    logtext(0,"RDBBuildAllDataMn");    BlockLoad(RDBr);  rwcnt = MatRowCnt(RDBr);    for (i=0;i<rwcnt;i=i+1) begin  	matrowget(RDBr,i,RDBrw);  	if(RDBrw.Valid==0)then begin      Logtext(0,"RDBBuildAllDataMn " & RDBrw.RegCode);      if(BuildRDBData(RDBrw.RegCode))then begin        RDBrw.Valid = 1;        matrowput(RDBr,i,RDBrw);        blockstore(RDBr);      end;		end;  end;    return;end;globalupdating procedure RDBDoClearDataMn()begin  Integer wn,i,rwcnt;  record RDBRegsBlock RDBr;  row RDBRegsBlock RDBrw;  row RDBRegsBlock RDBrw2;    BlockLoad(RDBr);  logtext(0,"RDBDoClearDataMn");  rwcnt = matrowcnt(RDBr);  for(i=0;i<rwcnt;i=i+1)begin    matrowget(RDBr,i,RDBrw);    if(RDBrw.Valid==0)then begin      logtext(0,"Clearing RDB " & RDBrw.RegCode);      ClearRDBData(RDBrw.RegCode)    end;  end;        return;end;